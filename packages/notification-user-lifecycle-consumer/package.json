{
  "name": "pagopa-interop-notification-user-lifecycle-consumer",
  "private": true,
  "version": "1.0.0",
  "license": "MPL-2.0",
  "description": "PagoPA Interoperability Notification User Lifecycle Consumer: a Kafka consumer that listens to messages from the Selfcare 'SC-Users' topic and stores the information about the user in a database",
  "main": "dist",
  "type": "module",
  "scripts": {
    "test": "vitest --config vitest.config.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json"
  },
  "keywords": [],
  "devDependencies": {
    "@pagopa/eslint-config": "catalog:",
    "@types/node": "20.19.4",
    "@types/pg": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "axios": "catalog:",
    "dotenv-flow": "catalog:",
    "drizzle-orm": "catalog:",
    "kafka-iam-auth": "workspace:*",
    "kafkajs": "catalog:",
    "pagopa-interop-api-clients": "workspace:*",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-commons-test": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "pagopa-interop-readmodel": "workspace:*",
    "pagopa-interop-readmodel-models": "workspace:*",
    "pg": "catalog:",
    "ts-pattern": "catalog:",
    "zod": "catalog:"
  }
}
