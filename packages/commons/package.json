{
  "name": "pagopa-interop-commons",
  "private": true,
  "version": "1.0.0",
  "license": "MPL-2.0",
  "description": "PagoPA Interoperability common utilities",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:autofix": "eslint . --fix",
    "build": "tsc && pnpm run copy-sql-files && cpx './src/pdf-generator/paged.polyfill.js' './dist/pdf-generator'",
    "check": "tsc --project tsconfig.check.json",
    "copy-sql-files": "cp ./src/repositories/sql/*.sql dist/repositories/sql"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "catalog:",
    "@aws-sdk/client-kms": "catalog:",
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/client-sesv2": "catalog:",
    "@aws-sdk/client-sqs": "catalog:",
    "@aws-sdk/s3-request-presigner": "catalog:",
    "@aws-sdk/client-dynamodb": "catalog:",
    "@aws-sdk/util-dynamodb": "catalog:",
    "@types/adm-zip": "catalog:",
    "@types/multer": "catalog:",
    "@zodios/core": "catalog:",
    "@zodios/express": "catalog:",
    "adm-zip": "catalog:",
    "axios": "catalog:",
    "connection-string": "catalog:",
    "date-fns": "catalog:",
    "date-fns-tz": "catalog:",
    "express": "catalog:",
    "fast-xml-parser": "catalog:",
    "handlebars": "catalog:",
    "jsonwebtoken": "catalog:",
    "jwks-rsa": "catalog:",
    "kafkajs": "catalog:",
    "mime": "catalog:",
    "multer": "catalog:",
    "nodemailer": "catalog:",
    "pagopa-interop-models": "workspace:*",
    "pg-promise": "catalog:",
    "puppeteer": "catalog:",
    "rate-limiter-flexible": "catalog:",
    "redis": "catalog:",
    "ts-pattern": "catalog:",
    "winston": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:",
    "zod-validation-error": "catalog:",
    "drizzle-orm": "catalog:"
  },
  "devDependencies": {
    "@types/express": "catalog:",
    "@types/jsonwebtoken": "catalog:",
    "@types/node": "catalog:",
    "@types/nodemailer": "catalog:",
    "cpx2": "catalog:",
    "eslint": "catalog:",
    "pagopa-interop-eslint-config": "workspace:*",
    "typescript": "catalog:"
  }
}
