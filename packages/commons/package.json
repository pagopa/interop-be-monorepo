{
  "name": "pagopa-interop-commons",
  "private": true,
  "version": "1.0.0",
  "license": "MPL-2.0",
  "description": "PagoPA Interoperability common utilities",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "build": "tsc && pnpm run copy-sql-files && cpx './src/pdf-generator/paged.polyfill.js' './dist/pdf-generator'",
    "copy-sql-files": "cp ./src/repositories/sql/*.sql dist/repositories/sql"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "10.1.1",
    "@aws-sdk/client-kms": "3.840.0",
    "@aws-sdk/client-s3": "3.842.0",
    "@aws-sdk/client-sesv2": "3.840.0",
    "@aws-sdk/client-sqs": "3.840.0",
    "@aws-sdk/s3-request-presigner": "3.842.0",
    "@aws-sdk/client-dynamodb": "3.840.0",
    "@aws-sdk/util-dynamodb": "3.840.0",
    "@types/adm-zip": "0.5.7",
    "@types/multer": "1.4.13",
    "@zodios/core": "10.9.6",
    "@zodios/express": "10.6.1",
    "adm-zip": "0.5.16",
    "axios": "1.12.0",
    "connection-string": "4.4.0",
    "date-fns": "3.6.0",
    "date-fns-tz": "3.1.3",
    "express": "4.20.0",
    "fast-xml-parser": "4.4.1",
    "handlebars": "4.7.8",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.1.0",
    "kafkajs": "2.2.4",
    "mime": "4.0.7",
    "multer": "2.0.2",
    "nodemailer": "7.0.11",
    "pagopa-interop-models": "workspace:*",
    "pg-promise": "11.8.0",
    "puppeteer": "22.11.2",
    "rate-limiter-flexible": "5.0.5",
    "redis": "4.6.15",
    "ts-pattern": "5.2.0",
    "winston": "3.16.0",
    "yaml": "2.4.5",
    "zod": "3.23.8",
    "zod-validation-error": "3.3.0",
    "drizzle-orm": "0.39.3"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "20.19.9",
    "@types/nodemailer": "6.4.17",
    "cpx2": "7.0.2",
    "eslint": "8.57.1",
    "prettier": "2.8.8",
    "typescript": "5.4.5"
  }
}