{
  "name": "pagopa-interop-backend-for-frontend",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "description": "PagoPA Interop Backend for Frontend",
  "main": "dist",
  "type": "module",
  "scripts": {
    "test:unit": "vitest",
    "test:api": "vitest --config vitest.api.config.ts",
    "test": "pnpm test:unit && pnpm test:api",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json"
  },
  "keywords": [],
  "devDependencies": {
    "@anatine/zod-mock": "catalog:",
    "@pagopa/eslint-config": "catalog:",
    "@types/adm-zip": "catalog:",
    "@types/express": "catalog:",
    "@types/node": "catalog:",
    "@types/supertest": "catalog:",
    "@types/swagger-ui-express": "catalog:",
    "eslint": "catalog:",
    "pagopa-interop-commons-test": "workspace:*",
    "prettier": "catalog:",
    "supertest": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "catalog:",
    "@aws-sdk/util-dynamodb": "catalog:",
    "@xmldom/xmldom": "catalog:",
    "@zodios/core": "catalog:",
    "@zodios/express": "catalog:",
    "adm-zip": "catalog:",
    "axios": "catalog:",
    "dotenv-flow": "catalog:",
    "express": "catalog:",
    "fast-xml-parser": "catalog:",
    "pagopa-interop-agreement-lifecycle": "workspace:*",
    "pagopa-interop-api-clients": "workspace:*",
    "pagopa-interop-application-audit": "workspace:*",
    "pagopa-interop-client-assertion-validation": "workspace:*",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "swagger-ui-express": "catalog:",
    "ts-pattern": "catalog:",
    "xml-crypto": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  }
}
