{
  "name": "audit-signer",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "description": "PagoPA Interoperability audit signer",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json"
  },
  "keywords": [],
  "devDependencies": {
    "@types/archiver": "6.0.2",
    "@types/adm-zip": "0.5.7",
    "@types/node": "20.19.4",
    "adm-zip": "0.5.10",
    "dotenv-flow": "4.1.0",
    "eslint": "9.26.0",
    "pagopa-interop-commons-test": "workspace:*",
    "prettier": "2.8.8",
    "tsx": "4.19.1",
    "typescript": "5.4.5",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.840.0",
    "@aws-sdk/client-sqs": "3.840.0",
    "archiver": "7.0.1",
    "axios": "1.12.0",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "zod": "3.23.8"
  }
