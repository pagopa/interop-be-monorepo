    "cpx2": "7.0.2",
    "@protobuf-ts/runtime": "2.9.4",
    "@types/express": "4.17.21",
    "@types/node": "20.19.3",
    "@types/supertest": "6.0.3",
    "date-fns": "3.6.0",
    "pagopa-interop-commons-test": "workspace:*",
    "pg-promise": "11.8.0",
    "prettier": "2.8.8",
    "supertest": "7.0.0",
    "testcontainers": "10.9.0",
    "tsx": "4.19.1",
    "typescript": "5.4.5",
