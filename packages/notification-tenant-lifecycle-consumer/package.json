    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "axios": "catalog:",
    "dotenv-flow": "catalog:",
    "kafka-iam-auth": "workspace:*",
    "kafkajs": "catalog:",
    "pagopa-interop-api-clients": "workspace:*",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "ts-pattern": "catalog:",
    "zod": "catalog:"
  }
}
