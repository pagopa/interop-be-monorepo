{
  "name": "pagopa-interop-notification-tenant-lifecycle-consumer",
  "private": true,
  "version": "1.0.0",
  "license": "MPL-2.0",
  "description": "PagoPA Interoperability tenant consumer service that creates/deletes notification configs for onboarded/deleted tenants",
  "main": "dist",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
