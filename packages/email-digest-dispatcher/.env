# Logger
LOG_LEVEL=info

# ReadModel SQL DB
READMODEL_SQL_DB_HOST=localhost
READMODEL_SQL_DB_PORT=6002
READMODEL_SQL_DB_NAME=root
READMODEL_SQL_DB_USERNAME=root
READMODEL_SQL_DB_PASSWORD=root
READMODEL_SQL_DB_USE_SSL=false

# Digest Tracking DB
DIGEST_TRACKING_DB_HOST=localhost
DIGEST_TRACKING_DB_PORT=6007
DIGEST_TRACKING_DB_NAME=root
DIGEST_TRACKING_DB_USERNAME=root
DIGEST_TRACKING_DB_PASSWORD=root
DIGEST_TRACKING_DB_USE_SSL=false
DIGEST_TRACKING_DB_SCHEMA=digest_tracking

# Digest frequency (hours between digest emails for same user)
DIGEST_FREQUENCY_HOURS=168

# Kafka Producer
PRODUCER_KAFKA_BROKERS=localhost:9092
PRODUCER_KAFKA_CLIENT_ID=email-notification-digest
PRODUCER_KAFKA_DISABLE_AWS_IAM_AUTH=true
PRODUCER_KAFKA_LOG_LEVEL=WARN

# Email Dispatch Topic
EMAIL_DISPATCH_TOPIC=email-dispatch.emails

# AWS (used by Kafka if IAM auth is enabled)
AWS_REGION=eu-south-1

# BFF URL for email deeplinks
BFF_URL=http://localhost:3000

# Priority producer IDs (comma-separated tenant UUIDs, shown first in new e-services digest)
PRIORITY_PRODUCER_IDS=bce8d16d-d26f-4c35-a835-35cca48ff8a5,2a9ad360-90ef-4499-ab2c-112b0fbcecc7,53b40136-65f2-424b-acfb-7fae17e35c60,27aaf0d7-850b-4aca-92b8-f4df70217448,86a2adeb-bf90-4248-b471-58ba3539f423,6c07e769-b418-4210-bc0f-7d56470a06e9,2e1c950b-26d6-471a-b5ec-bd233d5b1f07,87ac321d-f13f-49f3-8aa8-2aaae9dc2115
