{
  "name": "pagopa-interop-purpose-process",
  "version": "1.0.0",
  "description": "PagoPA Interoperability service for purpose",
  "main": "dist",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc && pnpm cpx './src/resources/**/*' './dist/resources'",
    "check": "tsc --project tsconfig.check.json"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@pagopa/eslint-config": "3.0.0",
    "@types/express": "4.17.21",
    "@types/node": "20.14.6",
    "cpx2": "7.0.1",
    "pagopa-interop-commons-test": "workspace:*",
    "prettier": "2.8.8",
    "puppeteer": "22.11.2",
    "testcontainers": "10.9.0",
    "tsx": "4.19.1",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@zodios/core": "10.9.6",
    "@zodios/express": "10.6.1",
    "axios": "1.7.4",
    "dotenv-flow": "4.1.0",
    "express": "4.20.0",
    "handlebars": "4.7.8",
    "mongodb": "6.7.0",
    "openapi-zod-client": "1.18.1",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-api-clients": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "pg-promise": "11.8.0",
    "ts-pattern": "5.2.0",
    "zod": "3.23.8"
  }
