{
  "name": "pagopa-interop-documents-generator",
  "private": true,
  "version": "1.0.0",
  "description": "PagoPA Interoperability documents generator",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc && pnpm cpx './src/resources/**/*' './dist/resources'",
    "check": "tsc --project tsconfig.check.json",
    "test": "vitest",
    "test:it": "vitest integration"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@pagopa/eslint-config": "3.0.0",
    "@types/node": "20.19.3",
    "cpx2": "7.0.2",
    "date-fns": "3.6.0",
    "pagopa-interop-commons-test": "workspace:*",
    "prettier": "2.8.8",
    "puppeteer": "22.11.2",
    "tsx": "4.19.1",
    "typescript": "5.4.5",
    "vitest": "1.6.1",
    "zlib": "1.0.5"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "2.9.4",
    "@zodios/core": "10.9.6",
    "connection-string": "4.4.0",
    "dotenv-flow": "4.1.0",
    "drizzle-orm": "0.39.3",
    "handlebars": "4.7.8",
    "kafka-iam-auth": "workspace:*",
    "kafkajs": "2.2.4",
    "mongodb": "6.7.0",
    "pagopa-interop-agreement-lifecycle": "workspace:*",
    "pagopa-interop-api-clients": "workspace:*",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "pagopa-interop-readmodel": "workspace:*",
    "pagopa-interop-readmodel-models": "workspace:*",
    "ts-pattern": "5.2.0",
    "zod": "3.23.8"
  }
}
