{
  "name": "pagopa-interop-api-clients",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "private": true,
  "description": "PagoPA Interop Api Clients",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:openapi": "spectral lint ./open-api/!\\(selfcareV2ClientApi.yml\\|authorizationManagementApi.yaml\\|notifierApi.yml\\|apiGatewayApi.yml\\) --ruleset ../../.spectral.yaml",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json",
    "generate-model": "tsx ./generate.ts",
    "clean-generated": "pnpm exec rm ./src/generated/*.ts"
  },
  "keywords": [],
  "devDependencies": {
    "@apidevtools/swagger-parser": "catalog:",
    "@pagopa/eslint-config": "catalog:",
    "eslint": "catalog:",
    "@stoplight/spectral-cli": "catalog:",
    "@types/node": "catalog:",
    "@types/qs": "catalog:",
    "dotenv-flow": "catalog:",
    "openapi-types": "catalog:",
    "handlebars": "catalog:",
    "openapi-zod-client": "catalog:",
    "openapi3-ts": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@zodios/core": "catalog:",
    "axios": "catalog:",
    "axios-logger": "catalog:",
    "pagopa-interop-commons": "workspace:*",
    "qs": "catalog:",
    "zod": "catalog:"
  }
}
