{
  "name": "pagopa-interop-api-clients",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "private": true,
  "description": "PagoPA Interop Api Clients",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:openapi": "spectral lint ./open-api/!\\(selfcareV2ClientApi.yml\\|authorizationManagementApi.yaml\\|notifierApi.yml\\|apiGatewayApi.yml\\) --ruleset ../../.spectral.yaml",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json",
    "generate-model": "tsx ./generate.ts && pnpm run generate-hey-api",
    "generate-hey-api": "openapi-ts",
    "clean-generated": "pnpm exec rm ./src/generated/*.ts"
  },
  "keywords": [],
  "devDependencies": {
    "@apidevtools/swagger-parser": "10.1.1",
    "@pagopa/eslint-config": "3.0.0",
    "eslint": "8.57.1",
    "@stoplight/spectral-cli": "6.15.0",
    "@types/node": "20.19.9",
    "@types/qs": "6.9.15",
    "dotenv-flow": "4.1.0",
    "openapi-types": "12.1.3",
    "handlebars": "4.7.8",
    "@hey-api/openapi-ts": "0.92.3",
    "openapi-zod-client": "1.18.3",
    "openapi3-ts": "3.1.0",
    "prettier": "2.8.8",
    "tsx": "4.19.1",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@zodios/core": "10.9.6",
    "axios": "1.12.0",
    "axios-logger": "2.8.1",
    "pagopa-interop-commons": "workspace:*",
    "qs": "6.14.1",
    "zod": "3.23.8",
    "@hey-api/client-fetch": "0.13.1"
  }
}
