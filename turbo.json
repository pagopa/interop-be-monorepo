{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "daemon": false,
  "globalDependencies": ["tsconfig.json"],
  "globalEnv": ["CI"],
  "tasks": {
    "start": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^build", "//#infra:start"]
    },
    "//#infra:start": {
      "cache": false
    },
    "//#infra:stop": {
      "cache": false
    },
    "//#infra:destroy": {
      "cache": false
    },
    "build": {
      "dependsOn": ["generate-model", "generate-protobuf", "^build"],
      "outputs": ["dist/**"]
    },
    "check": {
      "dependsOn": ["build"],
      "outputs": ["dist/**"]
    },
    "generate-model": {
      "dependsOn": ["^generate-model"],
      "outputs": ["src/generated/**"]
    },
    "generate-protobuf": {
      "dependsOn": ["^generate-protobuf"],
      "outputs": ["src/gen/**"]
    },
    "test": {
      "dependsOn": []
    },
    "test:integration": {
      "dependsOn": ["build"]
    },
    "test:api": {
      "dependsOn": ["build"]
    },
    "lint": {
      "dependsOn": [],
      "outputs": [".eslintcache"]
    },
    "lint:autofix": {
      "dependsOn": ["build"],
      "outputs": ["dist"]
    },
    "lint:openapi": {
      "dependsOn": [],
      "cache": false
    },
    "format:check": {
      "dependsOn": []
    },
    "format:write": {
      "dependsOn": []
    },
    "generate-db-models": {
      "dependsOn": ["//#infra:start", "^generate-db-models"],
      "outputs": ["src/generated/**"]
    }
  }
}