meta {
  name: Create EService Template Version
  type: http
  seq: 10
}

post {
  url: {{host-template}}/templates/:eserviceTemplateId/versions
  body: json
  auth: none
}

params:path {
  eserviceTemplateId: {{eserviceTemplateId}}
}

headers {
  Authorization: {{JWT}}
  X-Correlation-Id: {{correlation-id}}
}

vars:post-response {
  eserviceTemplateVersionId: res.body.id
}
