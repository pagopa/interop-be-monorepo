meta {
  name: Create Agreement
  type: http
  seq: 1
}

post {
  url: {{host-agreement}}/agreements
  body: json
  auth: none
}

headers {
  Authorization: {{JWT}}
  x-correlation-id: {{correlation-id}}
}

body:json {
  {
      "eserviceId":"{{eserviceId}}",
      "descriptorId":"{{descriptorId}}",
      "delegationId": "{{delegationId}}"
  }
}

vars:post-response {
  agreementId: res.body.id
}
