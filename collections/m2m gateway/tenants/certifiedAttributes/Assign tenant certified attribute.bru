meta {
  name: Assign tenant certified attribute
  type: http
  seq: 1
}

post {
  url: {{host-m2m-gw}}/tenants/:tenantId/certifiedAttributes
  body: json
  auth: none
}

params:path {
  tenantId: {{tenantId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
}

body:json {
  {
    "id" : "{{attributeId}}"
  }
}
