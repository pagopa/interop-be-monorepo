meta {
  name: Create EService Template versions
  type: http
  seq: 9
}

post {
  url: {{host-m2m-gw}}/eserviceTemplates/:templateId/versions
  body: json
  auth: none
}

params:path {
  templateId: {{eserviceTemplateId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
}

body:json {
  {
    "description": "Version description 2 ",
    "voucherLifespan": 100,
    "dailyCallsPerConsumer": 100,
    "dailyCallsTotal": 1000,
    "agreementApprovalPolicy": "AUTOMATIC"
  }
}

vars:post-response {
  eserviceTemplateVersionId: res.body.id
}

script:pre-request {
  const random = Math.round(Math.random() * 100)
  
  bru.setVar("randomName",`test name ${random}`)
}
