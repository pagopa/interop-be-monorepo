meta {
  name: Create eservice template version
  type: http
  seq: 9
}

post {
  url: {{host-m2m-gw}}/eserviceTemplates/:templateId/versions
  body: json
  auth: none
}

params:path {
  templateId: {{eserviceTemplateId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
}

vars:post-response {
  eserviceTemplateVersionId: res.body.id
}
