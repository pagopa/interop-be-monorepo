meta {
  name: List eservice template versions
  type: http
  seq: 7
}

get {
  url: {{host-m2m-gw}}/eserviceTemplates/:templateId/versions?offset=0&limit=10
  body: none
  auth: none
}

params:query {
  offset: 0
  limit: 10
}

params:path {
  templateId: {{eserviceTemplateId}}
}

headers {
  Authorization: {{JWT-M2M}}
}

vars:post-response {
  eserviceTemplateVersionId: res.body.results[0].id
}
