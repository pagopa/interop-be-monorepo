meta {
  name: List eservice template versions
  type: http
  seq: 2
}

get {
  url: {{host-m2m-gw}}/eserviceTemplates/:templateId/versions?offset=0&limit=10
  body: none
  auth: none
}

params:query {
  offset: 0
  limit: 10
}

params:path {
  templateId: {{templateId}}
}

headers {
  Authorization: {{JWT-M2M}}
}

vars:post-response {
  versionId: res.body.results.at(-1).id
}
