meta {
  name: Suspend eservice template version
  type: http
  seq: 10
}

post {
  url: {{host-m2m-gw}}/eserviceTemplates/:templateId/versions/:versionId/suspend
  body: json
  auth: none
}

params:path {
  versionId: {{eserviceTemplateVersionId}}
  templateId: {{eserviceTemplateId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
}

vars:post-response {
  eserviceTemplateVersionId: res.body.id
}
