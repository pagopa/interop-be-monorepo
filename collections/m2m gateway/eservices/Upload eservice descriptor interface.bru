meta {
  name: Upload eservice descriptor interface
  type: http
  seq: 6
}

post {
  url: {{host-m2m-gw}}/eservices/:eserviceId/descriptors/:descriptorId/interface
  body: multipartForm
  auth: none
}

params:path {
  eserviceId: {{eserviceId}}
  descriptorId: {{descriptorId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
  Content-Type: multipart/form-data
}

body:multipart-form {
  file: @file(/Users/ecamel/Downloads/interface.json)
  prettyName: Test File
}

vars:post-response {
  documentId: res.body.id
}
