meta {
  name: Update published eservice signal hub
  type: http
  seq: 1
}

patch {
  url: {{host-m2m-gw}}/eservices/:eserviceId/signalHub
  body: json
  auth: none
}

params:path {
  eserviceId: {{eserviceId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
  Content-Type: application/merge-patch+json
}

body:json {
  {
    "isSignalHubEnabled": true
  }
}
