meta {
  name: Update published eservice description
  type: http
}

patch {
  url: {{host-m2m-gw}}/eservices/:eserviceId/description
  body: json
  auth: none
}

params:path {
  eserviceId: {{eserviceId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
  Content-Type: application/merge-patch+json
}

body:json {
  {
    "description": "updated description"
  }
}
