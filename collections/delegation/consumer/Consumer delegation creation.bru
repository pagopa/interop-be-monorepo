meta {
  name: Consumer delegation creation
  type: http
  seq: 1
}

post {
  url: {{host-delegation}}/consumer/delegations
  body: json
  auth: none
}

headers {
  Authorization: {{JWT}}
  X-Correlation-Id: {{correlation-id}}
}

body:json {
  {
    "eserviceId": "{{eserviceId}}",
    "delegateId": "{{tenantId2}}"
  }
}

vars:post-response {
  delegationId: res.body.id
}
