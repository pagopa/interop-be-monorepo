meta {
  name: Get Purpose Template EServices
  type: http
  seq: 5
}

get {
  url: {{host-bff}}/purposeTemplates/:purposeTemplateId/eservices
  body: none
  auth: none
}

params:path {
  purposeTemplateId: {{purposeTemplateId}}
}

params:query {
  offset: 0
  limit: 10
  ~eserviceIds: ["{{eserviceId}}"]
  ~producerIds: ["{{tenantId}}"]
}

headers {
  Authorization: {{JWT}}
  X-Correlation-Id: {{correlation-id}}
}
