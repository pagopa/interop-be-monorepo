meta {
  name: Get Purpose Templates
  type: http
  seq: 3
}

get {
  url: {{host-purpose-template}}/purposeTemplates
  body: none
  auth: none
}

params:query {
  offset: 0
  limit: 10
  ~states: ["DRAFT", "ACTIVE"]
  ~eserviceIds: ["{{eserviceId}}"]
  ~creatorIds: ["{{tenantId}}"]
  ~purposeTitle: title
  ~targetTenantKind: PA
  ~excludeExpiredRiskAnalysis: false
  ~handlesPersonalData: false
}

headers {
  Authorization: {{JWT}}
  X-Correlation-Id: {{correlation-id}}
}
