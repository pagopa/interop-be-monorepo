meta {
  name: Create agreement
  type: http
  seq: 1
}

post {
  url: {{host-m2m-gw-v3}}/agreements
  body: json
  auth: none
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
}

body:json {
  {
      "eserviceId":"{{eserviceId}}",
      "descriptorId":"{{descriptorId}}"
  }
}

vars:post-response {
  agreementId: res.body.id
}
