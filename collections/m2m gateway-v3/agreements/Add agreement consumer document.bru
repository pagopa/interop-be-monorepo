meta {
  name: Add agreement consumer document
  type: http
  seq: 11
}

post {
  url: {{host-m2m-gw-v3}}/agreements/:agreementId/consumerDocuments
  body: multipartForm
  auth: none
}

params:path {
  agreementId: {{agreementId}}
}

headers {
  Authorization: {{DPOP-JWT-M2M-ADMIN}}
  DPoP: {{DPOP-PROOF}}
  Content-Type: multipart/form-data
}

body:multipart-form {
  file: @file()
  prettyName: Test File
}

vars:post-response {
  documentId: res.body.id
}
