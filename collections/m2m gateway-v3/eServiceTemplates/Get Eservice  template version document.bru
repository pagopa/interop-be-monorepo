meta {
  name: Get eservice template version document
  type: http
}

get {
  url: {{host-m2m-gw-v3}}/eserviceTemplates/:templateId/versions/:versionId/documents/:documentId
  body: none
  auth: none
}

params:path {
  versionId: {{versionId}}
  templateId: {{templateId}}
  documentId: {{documentId}}
}

headers {
  Authorization: {{DPOP-JWT-M2M}}
  DPoP: {{DPOP-PROOF}}
}
