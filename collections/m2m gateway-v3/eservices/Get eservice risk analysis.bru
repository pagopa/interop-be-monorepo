meta {
  name: Get eservice risk analyses
  type: http
}

get {
  url: {{host-m2m-gw-v3}}/eservices/:eserviceId/riskAnalyses/:riskAnalysisId
  body: none
  auth: none
}

params:path {
  eserviceId: {{eserviceId}}
  riskAnalysisId: {{riskAnalysisId}}
}

headers {
  Authorization: {{DPOP-JWT-M2M}}
  DPoP: {{DPOP-PROOF}}
}
