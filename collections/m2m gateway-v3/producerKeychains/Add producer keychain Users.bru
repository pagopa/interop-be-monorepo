meta {
  name: Add producerKeychain users
  type: http
}

post {
  url: {{host-m2m-gw-v3}}/producerKeychains/:producerKeychainId/users
  body: json
  auth: none
}

params:path {
  producerKeychainId: {{producerKeychainId}}
}

headers {
  Authorization: {{JWT-M2M-ADMIN}}
}

body:json {
  {
    "userIds": ["{{userId1}}", "{{userId2}}"]
  }
}
