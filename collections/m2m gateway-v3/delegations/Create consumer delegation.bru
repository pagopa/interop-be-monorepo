meta {
  name: Create consumer delegation
  type: http
  seq: 1
}

post {
  url: {{host-m2m-gw-v3}}/consumerDelegations
  body: json
  auth: none
}

headers {
  Authorization: {{DPOP-JWT-M2M-ADMIN}}
  DPoP: {{DPOP-PROOF}}
}

body:json {
  {
    "eserviceId": "{{eserviceId}}",
    "delegateId": "{{tenantId2}}"
  }
}

vars:post-response {
  delegationId: res.body.id
}
